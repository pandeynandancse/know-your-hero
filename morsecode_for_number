
#include <stdio.h>

int main()
{   int num,i,j;
    printf("Enter the number whose morse code u have to find : ");
    scanf("%d",&num);
    if(num < 6)
    {
        for(i =0 ; i< num;i++)
        {
            printf("O ");
            
        }
        for(j=0;j<5-num;j++)
        {
            printf("__ ");
        }
        
    }
    
    else
    {
        for(i =0 ; i< num-5;i++)
        {
            printf("__ ");
            
        }
        for(j=num-5;j<5;j++)
        {
            printf("O ");
        }
        
    }
    
    return 0;
}
